{
    "version":2,
    "builds":[{
        "src":"index.js","use":"@vercel/node"
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods":["GET","POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers":{
                "Access-Control-Allow-Origin": "https://blog-app1-beta.vercel.app"
            }
        }
    ]
}